#include<bits/stdc++.h>
using namespace std;

struct Node
{
    int data;
    Node* next;
};
 Node* head=NULL;
void Insert(int x, int indx)
{
   Node* temp=new Node();
   temp->data=x;
   if(indx==1)
   {
       temp->next=head;
       head=temp;
       return;
   }
   Node* temp2=head;
   for(int i=0; i<indx-2; i++)
   {
       temp2=temp2->next;
   }
   temp->next=temp2->next;
   temp2->next=temp;
}

void Print(Node *p)
{
    if(p==NULL)return;
   printf("%d ",p->data);
   Print(p->next);

}
int main()
{
    Insert(2,1); ///list is 2
    Insert(3,2); ///list is 2,3
    Insert(4,1);///list is 4,2,3
    Insert(5,2);///list is 4,5,2,3
    Print(head);
}
