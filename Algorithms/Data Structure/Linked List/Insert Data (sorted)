#include<bits/stdc++.h>
using namespace std;

struct Node
{
    int data;
    Node* next;
};

Node* Insert(int x, Node* head)
{
   if(head==NULL)
   {
       Node* temp=new Node();
       temp->data=x;
       temp->next=head;
       return temp;
   }
   else if(head->data>=x)
   {
       Node* temp=new Node();
       temp->data=x;
       temp->next=head;
       return temp;
   }
   head->next = Insert(x,  head->next);
   return head;
}



void Print(Node *p)
{
    if(p==NULL)return;
   printf("%d ",p->data);
   Print(p->next);

}
int main()
{
    Node* head=NULL;

    head=Insert(2,head); ///list is 2

    head=Insert(3, head); ///list is 2,3

    head=Insert(6, head);///list is 2,3,6

    head=Insert(5, head);///list is 2,3,5,6,
    head=Insert(1, head);///list is 1,2,3,5,6



    Print(head);
}
